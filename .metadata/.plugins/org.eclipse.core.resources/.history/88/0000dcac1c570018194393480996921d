	.short 1608
	.short 32757
	.short 804

	@24:sign
	@20:j
	@16:s=[0:N/4-1]
	@12:blk_exp
	@8:R=1
	@4:S=N
	@0:stage,m
.section .text, "ax"
.align 4
.global Complex_FFT_256
Complex_FFT_256:
	stmfd r13!,{r4-r11,r14}
	sub r13,r13,#28
	mov r4,#256 @r4=n
	mov r5,#1 @r5=R=1
	mov r6,#0 @r6=m=0
	str r4,[r13,#4]
	str r5,[r13,#8]
	str r0,[r13,#24]
	str r6,[r13]
	mov r12,#256
	mov r4,r0 @r4=frfi
	ldr r6,[r4],#4 @r6={real,imag}
	mov r5,#3 @r5=sign_bits
L1:
	eor r7,r6,r6,lsl #1 @imag
	clz r8,r7
	cmp r5,r8
	movgt r5,r8
	mov r7,r6,lsl #16 @r7={16'b0,real[15:0]}
	eor r7,r7,r7,lsl #1
	clz r8,r7
	cmp r5,r8
	movgt r5,r8
	ldr r6,[r4],#4
	subs r12,r12,#1
	bgt L1

	mov r12,#256
	mov r6,#32768
	cmp r5,#3 @sign_bits<3?
	mov r4,r0
	ldr r7,[r4] @r7={real[15:0],imag[15:0]}
	ldrlt r6,[r15,r5,lsl #2]
	b L2
	.word 4096
	.word 8192
	.word 16384
L2:
	smulbb r8,r7,r6 @real*s,q30
	smultb r9,r7,r6 @imag*s,q30
	qadd r8,r8,r8 @q31
	qadd r9,r9,r9 @q31
	mov r8,r8,lsr #16
	mov r9,r9,lsr #16
	orr r8,r8,r9,lsl #16
	str r8,[r4],#4
	subs r12,r12,#1
	ldr r7,[r4]
	bgt L2

	rsb r6,r5,#3
	str r6,[r13,#12]
	mov r4,#0 @m=0
	str r4,[r13]
L3:
	mov r1,#0
	str r1,[r13,#16] @s=0
	mov r0,#3 @r0=sign
L4:
	ldr r8,[r13,#24]
	ldr r5,[r13,#8] @r5=R
	ldr r6,[r13,#4] @r6=S
	str r5,[r13,#20] @j=R
	mul r7,r1,r5 @s*R
	add r1,r8,r1,lsl #2 @r1=&frfi[s]
	add r2,r1,r6 @r2=&frfi[N/4+s]
	add r3,r2,r6 @r3=&frfi[N/2+s]
	add r4,r3,r6 @r4=&frfi[3*N/4+s]
	ldr r6,=CTable
	add r5,r6,r7,lsl #2 @&table[s*R]
	add r8,r5,r7,lsl #2 @&table[2*s*R]
	add r9,r8,r7,lsl #2 @&table[3*s*R]
	ldr r6,[r5] @y,r6=W(s,m=1)
	ldr r5,[r8] @x,r5=W(s,m=2)
	ldr r7,[r9] @z,r7=W(s,m=3)
L5:
	ldr r8,[r1] @r8={a.real,a.imag}
	ldr r9,[r3] @r9={c.real,c.imag}
	add r10,r8,r9 @r10=a
	sub r11,r8,r9 @r11=c
	ldr r8,[r2] @r8={b.real,b.imag}
	ldr r9,[r4] @r9={d.real,d.imag}
	add r12,r8,r9 @r12=b
	sub r14,r8,r9 @r14=d
	add r8,r10,r12 @r8=a
	sub r9,r10,r12 @r9=b
	str r8,[r1]
	eor r10,r8,r8,lsl #1
	clz r10,r10
	cmp r0,r10
	movgt r0,r10
	mov r8,r8,lsl #16
	eor r10,r8,r8,lsl #1
	clz r10,r10
	cmp r0,r10
	movgt r0,r10
	smulbb r10,r9,r5 @b.real*x.real
	smultt r12,r9,r5 @b.imag*x.imag
	qsub r10,r10,r12
	smultb r12,r9,r5 @r12=b.imag*x.real
	smlabt r12,r9,r5,r12 @r12+=b.real*x.imag
	qadd r10,r10,r10
	qadd r12,r12,r12
	mov r10,r10,lsr #16 @real
	mov r12,r12,lsr #16 @imag
	orr r10,r10,r12,lsl #16
	str r10,[r2] @-j*(a+jb)=(b-j*a)
	eor r8,r10,r10,lsl #1
	clz r8,r8
	cmp r0,r8
	movgt r0,r8
	mov r10,r10,lsl #16
	eor r8,r10,r10,lsl #1
	clz r8,r8
	cmp r0,r8
	movgt r0,r8
	mov r10,r14,lsl #16 @{16'b0,d.real}
	mov r10,r10,asr #16 @{d.real,16{d.real[15]}}
	rsb r10,r10,#0 @-real
	mov r10,r10,lsl #16 @-j*(a+jb)=b-j*a
	orr r14,r10,r14,lsr #16 @r14=-j*d
	add r8,r11,r14 @c+d
	sub r9,r11,r14 @c-d
	smulbb r10,r8,r6 @c.real*y.real
	smultt r12,r8,r6 @c.imag*y.imag
	qsub r10,r10,r12
	smultb r12,r8,r6
	smlabt r12,r8,r6,r12
	qadd r10,r10,r10
	qadd r12,r12,r12
	mov r10,r10,lsr #16
	mov r12,r12,lsr #16
	orr r10,r10,r12,lsl #16
	str r10,[r3]
	eor r8,r10,r10,lsl #1
	clz r8,r8
	cmp r0,r8
	movgt r0,r8
	mov r10,r10,lsl #16
	eor r8,r10,r10,lsl #1
	clz r8,r8
	cmp r0,r8
	movgt r0,r8
	smulbb r10,r9,r7 @d.real*z.real
	smultt r12,r9,r7 @d.imag*z.imag
	qsub r10,r10,r12
	smultb r12,r9,r7 @d.imag*z.real
	smlabt r12,r9,r7,r12 @d.real*z.imag
	qadd r10,r10,r10
	qadd r12,r12,r12
	mov r10,r10,lsr #16
	mov r12,r12,lsr #16
	orr r10,r10,r12,lsl #16
	str r10,[r4]
	eor r8,r10,r10,lsl #1
	clz r8,r8
	cmp r0,r8
	movgt r0,r8
	mov r10,r10,lsl #16
	eor r8,r10,r10,lsl #1
	clz r8,r8
	cmp r0,r8
	movgt r0,r8
	ldr r8,[r13,#20] @r8=j
	ldr r9,[r13,#4]
	subs r8,r8,#1
	str r8,[r13,#20]
	add r1,r1,r9,lsl #2 @a+=S
	add r2,r2,r9,lsl #2 @b+=S
	add r3,r3,r9,lsl #2 @c+=S
	add r4,r4,r9,lsl #2 @d+=S

	bgt L5

	ldr r1,[r13,#16] @r1=s
	ldr r6,[r13,#4] @r6=S
	add r1,r1,#1
	cmp r1,r6,asr #2 @s<S/4?
	str r1,[r13,#16] @s+=1
	blt L4
	ldr r4,[r13]
	cmp r4,#3
	beq L7

	mov r12,#256
	mov r5,#32768
	ldr r1,[r13,#24] @r1=frfi
	cmp r0,#3 @sign_bits
	ldr r6,[r1]
	ldrlt r5,[r15,r0,lsl #2]
	b L6
	.word 4096
	.word 8192
	.word 16384
L6:
	smulbb r11,r5,r6
	smulbt r10,r5,r6
	qadd r11,r11,r11
	qadd r10,r10,r10
	mov r11,r11,lsr #16
	mov r10,r10,lsr #16
	orr r11,r11,r10,lsl #16
	str r11,[r1],#4
	subs r12,r12,#1
	ldr r6,[r1]
	bgt L6

	ldr r5,[r13,#4]
	ldr r6,[r13,#8]
	mov r5,r5,asr #2
	mov r6,r6,lsl #2
	str r5,[r13,#4]
	str r6,[r13,#8]
	ldr r5,[r13,#12] @blk_expnts
	rsb r0,r0,#3
	add r5,r5,r0
	str r5,[r13,#12]
	cmp r4,#4
	add r4,r4,#1
	str r4,[r13]
	blt L3
L7:
	mov r12,#0
	ldr r8,[r13,#24]
L8:
	mov r0,r12
	mov r1,#0
	bl permutate
	mov r1,#1
	bl permutate
	mov r1,#2
	bl permutate
	mov r1,#3
	bl permutate
	mov r1,#4
	bl permutate
	mov r0,r0,lsr #24
	cmp r0,r12
	addgt r9,r8,r12,lsl #2
	addgt r7,r8,r0,lsl #2
	ldrgt r10,[r9]
	ldrgt r11,[r7]
	strgt r10,[r7]
	strgt r11,[r9]
	add r12,r12,#1
	cmp r12,#256
	blt L8
	ldr r0,[r13,#12]
	add r13,r13,#28
	ldmfd r13!,{r4-r11,r15}

permutate:
	sub r13,r13,#4
	str r4,[r13]
	ldr r2,=0x0000ffff
	cmp r1,#4
	ldrlt r2,[r15,r1,lsl #2]
	b L9
	.word 0x55555555
	.word 0x33333333
	.word 0x0f0f0f0f
	.word 0x00ff00ff
L9:
	mov r3,#1
	mov r3,r3,lsl r1
	mov r4,r0,asr r3
	and r4,r2,r4
	and r0,r0,r2
	orr r0,r4,r0,lsl r3
	add r13,r13,#4
	bx r14
.end
